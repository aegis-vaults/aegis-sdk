{
  "name": "@aegis-vaults/sdk",
  "version": "0.2.1",
  "description": "TypeScript SDK for Aegis Protocol - On-chain operating system for AI finance on Solana",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./agents": {
      "types": "./dist/agents/index.d.ts",
      "import": "./dist/agents/index.js",
      "require": "./dist/agents/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:devnet": "tsx examples/devnet-test.ts",
    "verify:setup": "tsx examples/verify-setup.ts",
    "example:quickstart": "tsx examples/quickstart.ts",
    "example:devnet": "tsx examples/devnet-test.ts",
    "example:transfer": "tsx examples/transfer-example.ts",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "solana",
    "aegis",
    "ai",
    "finance",
    "vault",
    "policy",
    "sdk",
    "typescript",
    "blockchain",
    "web3"
  ],
  "author": "Aegis Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/aegis-vaults/aegis-sdk"
  },
  "bugs": {
    "url": "https://github.com/aegis-vaults/aegis-sdk/issues"
  },
  "homepage": "https://github.com/aegis-vaults/aegis-sdk#readme",
  "dependencies": {
    "@coral-xyz/anchor": "^0.30.1",
    "@solana/web3.js": "^1.87.0",
    "@types/bn.js": "^5.2.0",
    "bn.js": "^5.2.2",
    "borsh": "^0.7.0",
    "bs58": "^5.0.0",
    "cross-fetch": "^4.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.1",
    "tsx": "^4.21.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "@solana/web3.js": "^1.87.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
